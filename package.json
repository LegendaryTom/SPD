{
    "name": "PV_system_drawing",
    "version": "",
    "author": "",
    "description": "",

    "browserify": {
        "transform": [ "browserify-shim" ]
    },
    "dependencies": {
        "underscore": "latest",
        "moment": "latest",
        "snapsvg": "latest",
        "brfs": "1.0.2",
        "pdfkit": "latest",
        "jquery": "latest"
    },
    "devDependencies": {
        "browserify": "^5.11.0",
        "browserify-shim": "^3.7.0",
        "beefy": "latest"
    },
    "browser": {
        "jsPDF": "./lib/jspdf",
        "FileSaver": "./lib/FileSaver",
        "svgToPdf": "./lib/svgToPdf"

    },
    "browserify-shim": {
        "FileSaver": {
            "exports": "saveAs"
